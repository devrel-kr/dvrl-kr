{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "LetsEncrypt": {
      "matchCondition": {
        "route": "/.well-known/acme-challenge/{challengeCode}"
      },
      "backendUri": "%AzureWebJobsStorageLetsEncryptChallengeUrl%/.well-known/acme-challenge/{challengeCode}"
    },
    "Redirect": {
      "matchCondition": {
        "route": "/{shortUrl}"
      },
      "responseOverrides": {
        "response.statusCode": "301",
        "response.statusReason": "Moved Permanently",
        "response.headers.location": "/-/{shortUrl}"
      }
    },
    "Root": {
      "matchCondition": {
        "route": "/"
      },
      "responseOverrides": {
        "response.statusCode": "301",
        "response.statusReason": "Moved Permanently",
        "response.headers.location": "%DefaultRedirectUrl%"
      }
    }
  }
}
